<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.BooksaleEntity" table="booksale" schema="" catalog="reg">
        <id name="sid">
            <column name="sid" sql-type="bigint" not-null="true"/>
        </id>
        <property name="bid">
            <column name="bid" sql-type="bigint"/>
        </property>
        <property name="quantity">
            <column name="quantity" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>