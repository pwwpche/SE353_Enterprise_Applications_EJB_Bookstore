<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.SaleEntity" table="sale" schema="" catalog="reg">
        <id name="sid">
            <column name="sid" sql-type="bigint" not-null="true"/>
        </id>
        <property name="uid">
            <column name="uid" sql-type="bigint"/>
        </property>
        <property name="sYear">
            <column name="s_year" sql-type="int"/>
        </property>
        <property name="sMonth">
            <column name="s_month" sql-type="int"/>
        </property>
        <property name="sDay">
            <column name="s_day" sql-type="int"/>
        </property>
        <property name="totprice">
            <column name="totprice" sql-type="decimal" precision="7" scale="2"/>
        </property>
    </class>
</hibernate-mapping>